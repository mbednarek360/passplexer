digraph d {

    // settings
    graph [fontsize=16 compound=true, splines=ortho];
    node [shape=rect fontsize=14, style=filled];

    // inputs
    subgraph cluster_input {

        // label
        label = "Input";

        // input nodes
        s [label = "Service"];
        u [label = "Username"];
        p [label = "Password"];


    }

    // hashes
    subgraph cluster_hash {

        // hash nodes
        sh [label = "SHA-1"];
        uh [label = "SHA-1"];
        ph [label = "SHA-1"];

        // hash inputs
        s -> {sh, uh}
        u -> {uh, ph}
        p -> {ph, sh}

        // interlace
        i [label = "Interlace"];
        sh -> i
        uh -> i
        ph -> i

        // final hash
        fh [label = "SHA3-256"];
        i -> fh
    }

    // output
    subgraph cluster_output {

        // label
        label = "Output"
        labelloc="b"

        // encoding
        out [label = "Base 91"];
        fh -> out;
    }
}